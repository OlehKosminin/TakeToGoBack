{
    "compilerOptions": {
        "target": "es5",
        "module": "CommonJS",
        "outDir": "dist",
        "rootDir": "src",
        "strict": true,
        "isolatedModules": true,
        "esModuleInterop": true,
        "allowJs": true,
        "skipLibCheck": true,
        "allowSyntheticDefaultImports": true,
        "lib": [
            "dom",
            "dom.iterable",
            "esnext"
        ],
    }
}